<?xml version="1.0" encoding="UTF-8"?>
<vxml xmlns="http://www.w3.org/2001/vxml" version="2.0">
    <form id="main">   
        <grammar src="file" />
        <initial name="Start">
            <prompt>Vítejte v registraci p?edm?t?</prompt>
            <filled>
                <assign name="Start" expr="true" />
            </filled>
            <nomatch count="3">
                <exit />
            </nomatch>
            <noinput count="3">
                <exit />
            </noinput>
            <grammar src="Start" />
        </initial>
        <field name="Finishing">
            <prompt>Zadejte zp?sob ukon?ení.</prompt>
            <prompt count="2">Možnosti jsou zkouška nebo zápo?et.</prompt>
            <nomatch count="3">
                <exit />
            </nomatch>
            <noinput count="3">
                <exit />
            </noinput>
            <grammar src="Finishing" />
        </field>
        <field name="Course">
            <prompt>Zadejte registrovaný p?edm?t.</prompt>

            <prompt count="2">Lze si zaregistrovat p?edm?ty PB095, PB125, PB162 nebo PA156.</prompt>
            <nomatch count="3">
                <exit />
            </nomatch>
            <noinput count="3">
                <exit />
            </noinput>
            <grammar src="Course" />
        </field>
        <field name="All">
            <prompt>Zadal jste <value expr="Course" />, ukon?ený <value expr="Finishing" />. Souhlasíte?</prompt>
            <prompt count="2">Ano nebo ne?</prompt>
            <nomatch count="3">
                <exit />
            </nomatch>
            <noinput count="3">
                <exit />
            </noinput>
            <filled>
                <if expr="All=='ne'">
                    <clear namelist="Start Finishing Course All" />
                </if>
            </filled>
            <grammar src="All" />
        </field>
        <field name="Submit">
            <submit src="uri" namelist="Course Finishing" />
            <nomatch count="3">
                <exit />
            </nomatch>
            <noinput count="3">
                <exit />
            </noinput>
            <grammar src="Submit" />
        </field>
    </form>
</vxml>